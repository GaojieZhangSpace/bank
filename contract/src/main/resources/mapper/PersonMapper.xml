<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://www.mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.demo.mapper.PersonMapper">
	<!-- 新增信息到人员表 -->
	<insert id="addPerson">
		insert into
		person(record,borrower,lender,bPlace,lPlace,bTel,lTel,bAgent,lAgent,bIsCom,lIsCom)values(#{record},#{borrower},#{lender},#{bPlace},#{lPlace},#{bTel},#{lTel},#{bAgent},#{lAgent},#{bIsCom},#{lIsCom})
	</insert>
</mapper>